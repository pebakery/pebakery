[Main]
Title=ControlGeneral
Author=ied206
Description=UnitTest
Version=001
Level=5
Selected=True
Mandatory=False

[TestVars]
%A%=1
%B%=2
%C%=3
InlineMacro=Set,#r,T

[TestMacro]
SectionMacro=Run,%ScriptFile%,SectionMacroImpl
InlineMacro=Set,#r,#1

[SectionMacroImpl]
Set,#r,#1

[Process-SectionMacro-Global]
AddVariables,%ScriptFile%,TestMacro,GLOBAL
Set,#r,F
SectionMacro,T

[Process-SectionMacro-Local]
AddVariables,%ScriptFile%,TestMacro
Set,#r,F
SectionMacro,T

[Process-InlineMacro-Global]
AddVariables,%ScriptFile%,TestMacro,GLOBAL
Set,#r,F
InlineMacro,T

[Process-InlineMacro-Local]
AddVariables,%ScriptFile%,TestMacro
Set,#r,F
InlineMacro,T

[Process-GetParam00]
Run,%ScriptFile%,GetParam,0,""

[Process-GetParam01]
Run,%ScriptFile%,GetParam,1,"P1"

[Process-GetParam09]
Run,%ScriptFile%,GetParam,9,"P9"

[Process-GetParam12]
Run,%ScriptFile%,GetParam,12,"P12"

[Process-GetParam16]
Run,%ScriptFile%,GetParam,16,"P16"

[Process-GetParam18]
Run,%ScriptFile%,GetParam,18,""

[GetParam]
Set,%Dest%,F
Set,%Index%,#1
Run,%ScriptFile%,ReadParam,P1,P2,P3,P4,P5,P6,P7,P8,P9,P10,P11,P12,P13,P14,P15,P16
If,%Result%,Equal,#2,Set,%Dest%,T

[ReadParam]
GetParam,%Index%,%Result%

[Process-Exit01]
Set,#r,T
Exit,"##r must be T"
Set,#r,F

[Process-Exit02]
System,SetLocal
Set,#r,T
Exit,"##r must be T"
Set,#r,F

[Process-Return01]
Set,#r,INIT
Run,%ScriptFile%,"Process-SubReturn-NoValue"
Set,%Dest%,"%Dest%T"

[Process-Return02]
Set,#r,INIT
Run,%ScriptFile%,"Process-SubReturn-NoValue",True
Set,%Dest%,"%Dest%T"

[Process-Return03]
Set,#r,INIT
Run,%ScriptFile%,"Process-SubReturn-WithValue"
Set,%Dest%,"%Dest%T"

[Process-Return04]
Set,#r,INIT
Run,%ScriptFile%,"Process-SubReturn-WithValue",True
Set,%Dest%,"%Dest%T"

[Process-SubReturn-NoValue]
Set,%Dest%,T
Set,#r,SUB
Return
Set,%Dest%,F

[Process-SubReturn-WithValue]
Set,%Dest%,T
Set,#r,SUB
Return,#1
Set,%Dest%,F

[Process-Halt-Simple]
Set,#r,T
Halt
Set,#r,F

[Process-Halt-InsideIf]
Set,%i%,True
If,%i%,Equal,True,Begin
  Set,#r,T
  Halt
  Set,#r,F
End
